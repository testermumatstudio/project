{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true
  },
  "baseUrl": "src",
  "jsx": "react",    
  "experimentalDecorators": true,
  "paths":  {
    "@assets/*": ["assets/*"],
    "@components/*": ["components/*"],
    "@config/*": ["config/*"],
    "@utils/*": ["utils/*"],
    "@img/*": ["img/*"],
    "@models/*": ["models/*"],
    "@navigation/*": ["navigation/*"],
    "@screens/*": ["screens/*"],
    "@stores/*": ["stores/*"],
    "@context": ["context/*"],
    "hooks": ["hooks/*"],
    "schemas-validation": ["schemas-validation/*"],
    "screens": ["screens/*"],
    "@*": ["*"]
  }
}
